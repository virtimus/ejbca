# Since selenium cannot inject SSL certificate using the web driver, predefined web browser profiles is required.
# By default, the first certificate available in each browser profile will be used. E.g. SuperAdmin.p12

# Administrator roles which corresponding admins are member of MUST have the same name as browser profile below.

profile.firefox.default = default

profile.firefox.superadmin = superadmin
profile.firefox.raadmin = raadmin
profile.firefox.raadminalt = raadminalt
profile.firefox.caadmin = caadmin
profile.firefox.auditor = auditor
profile.firefox.custom = custom
profile.firefox.supervisor = supervisor